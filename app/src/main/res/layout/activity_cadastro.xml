<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/back"

    tools:context=".Cadastro">


    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@android:string/autofill"
        tools:layout_editor_absoluteX="22dp"
        tools:layout_editor_absoluteY="39dp" />

    <EditText
        android:id="@+id/edtTipo"
        android:layout_width="371dp"
        android:layout_height="40dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="@string/lbl_tipo"
        tools:layout_editor_absoluteX="26dp"
        tools:layout_editor_absoluteY="78dp"         android:textColor="@color/Vermelho"
    />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Descrição"
        tools:layout_editor_absoluteX="27dp"
        android:textColor="@color/Vermelho"
        tools:layout_editor_absoluteY="137dp" />

    <EditText
        android:id="@+id/edtTelefone"
        android:layout_width="364dp"
        android:layout_height="133dp"
        android:layout_marginEnd="24dp"
        android:layout_marginBottom="424dp"
        android:ems="10"
        android:textColor="@color/Vermelho"
        android:hint="@string/lbl_descricao"
        android:inputType="textPersonName"
        android:text="@string/lbl_descricao"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TextView"
        tools:layout_editor_absoluteX="31dp"
        android:textColor="@color/Vermelho"
        tools:layout_editor_absoluteY="322dp" />

    <EditText
        android:id="@+id/editText3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:inputType="textPersonName"
        android:textColor="@color/Vermelho"
        android:text="@string/lbl_latlng"
        tools:layout_editor_absoluteX="27dp"
        tools:layout_editor_absoluteY="363dp" />

    <Button
        android:id="@+id/button2"
        android:layout_width="wrap_content"
        android:textColor="@color/Vermelho"
        android:layout_height="wrap_content"
        android:text="Button"
        tools:layout_editor_absoluteX="294dp"
        tools:layout_editor_absoluteY="466dp" />
</androidx.constraintlayout.widget.ConstraintLayout>